                                    <div class="item active">
                                        <ul class="thumbnails">
                                        {{#each categories}}
                                            <li class="span3">
                                                <div class="thumbnail">
                                                    <i class="tag"></i>
                                                    <a href="/product_details?id={{id}}"><img src="/{{{image_link}}}" alt=""></a>
                                                    <div class="caption">
                                                        <h5>{{name}}</h5>
                                                        <h4><a class="btn" href="/product_details?id={{id}}">Xem</a> <span class="pull-right">{{price}}</span></h4>
                                                        <p>Ngày ra mắt: {{created}}</p>
                                                    </div>
